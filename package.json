{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run clean && webpack --progress -p --config webpack.production.config.js",
    "start": "webpack-dev-server --progress --history-api-fallback --hot --inline --port 9000 --open",
    "dev": "npm run start",
    "dev:remote": "webpack-dev-server --progress --history-api-fallback --hot --inline --host 0.0.0.0 --port 9000 --open",
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "eslint ./app",
    "createServer": "cd dist && npm init -f && npm i express -S"
  },
  "author": "Luke Andersen",
  "license": "ISC",
  "dependencies": {
    "axios": "1.13.5",
    "classnames": "2.5.1",
    "express": "4.22.1",
    "flexboxgrid": "6.3.1",
    "normalize.css": "8.0.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.1",
    "react-router-dom": "5.3.4"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.13.10",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.2",
    "postcss": "8.5.6",
    "postcss-cssnext": "3.1.1",
    "postcss-import": "12.0.1",
    "postcss-load-config": "3.1.4",
    "postcss-loader": "3.0.0",
    "postcss-nesting": "7.0.1",
    "rimraf": "3.0.2",
    "style-loader": "1.3.0",
    "url-loader": "4.1.1",
    "webpack": "4.47.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "5.2.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions",
          "> 5%"
        ]
      },
      "postcss-nesting": {}
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "indent": [
        0,
        4
      ],
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": 0,
      "react/jsx-indent": [
        2,
        4
      ],
      "react/jsx-indent-props": [
        2,
        4
      ],
      "react/require-default-props": 0,
      "max-len": [
        1,
        100,
        4,
        {
          "ignorePattern": "\\s*<"
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": [
        "error",
        {
          "caseSensitive": false
        }
      ],
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/require-extension": 0,
      "no-console": 0,
      "react/no-string-refs": 1,
      "no-unused-vars": [
        2,
        {
          "varsIgnorePattern": "[iI]gnored"
        }
      ],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "react/forbid-prop-types": 0,
      "jsx-a11y/img-has-alt": 0,
      "jsx-a11y/label-has-for": [
        2,
        {
          "required": {
            "every": [
              "id"
            ]
          }
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ]
    }
  }
}
